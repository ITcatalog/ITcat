     -->

    


    <!-- http://schema.org/employee -->

    <owl:AnnotationProperty rdf:about="&schema;employee"/>
    


    <!-- http://www.purl.org/dc/terms/abstract -->

    <owl:AnnotationProperty rdf:about="&dcterms;abstract">
        <rdfs:comment xml:lang="de">Diese Standardrelation wird in dieser Ontologie verwendet, um Kurzbeschreibungen von IT-Diensten bereitzustellen, die als Teaser im IT-Katalog erscheinen.</rdfs:comment>
        <skos:prefLabel xml:lang="de">Kurzbeschreibung</skos:prefLabel>
        <skos:definition xml:lang="de">Relation zu einer textuellen Kurzbeschreibung einer Ressource.</skos:definition>
        <skos:prefLabel xml:lang="en">abstract</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://www.purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="&dcterms;description">
        <skos:prefLabel xml:lang="de">Beschreibung</skos:prefLabel>
        <rdfs:comment xml:lang="de">Diese Standardrelation wird in dieser Ontologie verwendet, um Beschreibungen von IT-Diensten bereitzustellen.</rdfs:comment>
        <skos:definition xml:lang="de">Im engeren Sinne: Relation zu einer textuellen Beschreibung einer Ressource. Kann im weiteren Sinne auch für andere Formen von Beschreibungen genutzt werden: Grafiken, Inhaltsverzeichnissen etc.</skos:definition>
        <skos:prefLabel xml:lang="en">description</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://www.purl.org/dc/terms/subject -->

    <owl:AnnotationProperty rdf:about="&dcterms;subject">
        <rdfs:comment xml:lang="de">Diese Standardrelation wird in dieser Ontologie verwendet, um Schlüsselwörter zu IT-Diensten zu referenzieren.</rdfs:comment>
        <skos:definition xml:lang="de">Ein thematischer Gegenstand einer Ressource.</skos:definition>
        <skos:prefLabel xml:lang="de">Gegenstand</skos:prefLabel>
        <skos:prefLabel xml:lang="en">subject</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://www.purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="&dcterms;title">
        <skos:prefLabel xml:lang="de">Bezeichnung</skos:prefLabel>
        <skos:definition xml:lang="de">Die vollständige (offizielle) Bezeichnung / der Name einer Ressource.</skos:definition>
        <rdfs:comment xml:lang="de">Diese Standardrelation wird in dieser Ontologie verwendet, um vollständige Bezeichnungen von IT-Diensten bereitzustellen.</rdfs:comment>
        <skos:prefLabel xml:lang="en">title</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="&skos;altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="&skos;definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="&skos;prefLabel">
        <rdfs:comment xml:lang="de">Diese Standardrelation wird in dieser Ontologie verwendet, um primäre Bezeichnungen von Ressourcen bereitzustellen.</rdfs:comment>
        <skos:prefLabel xml:lang="de">Hauptbezeichnung</skos:prefLabel>
        <rdfs:comment xml:lang="de">In jeder Sprache kann es immer nur ein skos:prefLabel geben.</rdfs:comment>
        <skos:definition xml:lang="de">Standardrelation zur Auszeichnung einer Bevorzugten Bezeichnung einer Ressource.</skos:definition>
        <skos:prefLabel xml:lang="en">preferred label</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://xmlns.com/foaf/0.1/mbox -->

    <owl:AnnotationProperty rdf:about="&foaf;mbox"/>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:AnnotationProperty rdf:about="&foaf;name"/>
    


    <!-- http://xmlns.com/foaf/0.1/phone -->

    <owl:AnnotationProperty rdf:about="&foaf;phone"/>
    


