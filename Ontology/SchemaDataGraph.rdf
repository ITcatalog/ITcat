    <!-- http://th-brandenburg.de/ns/itcat#BRZUR -->

    <owl:NamedIndividual rdf:about="&itcat;BRZUR">
        <rdf:type rdf:resource="&foaf;Organization"/>
        <skos:prefLabel xml:lang="de">BRZ Uni Regensburg</skos:prefLabel>
        <dcterms:title xml:lang="de">Bibliotheksrechenzentrum der Universität Regensburg</dcterms:title>
        <skos:prefLabel xml:lang="en">University Data Center Regensburg</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://th-brandenburg.de/ns/itcat#BSCW -->

    <owl:NamedIndividual rdf:about="&itcat;BSCW">
        <rdf:type rdf:resource="&schema;Service"/>
        <dcterms:subject></dcterms:subject>
        <rdfs:comment></rdfs:comment>
        <dcterms:description xml:lang="de">BSCW ist ein Projekt- und Dokumentenmanagementsystem (Basic Support for Cooperative Work). Es wurde vor 20 Jahren vom Fraunhofer Institut FIT entwickelt. Inzwischen zeichnet die OrbiTeam Software GmbH &amp; Co. KG für die Pflege, Weiterentwicklung und den Support des Systems verantwortlich. Der FB Wirtschaft hat eine Lizenz für Forschung und Lehre erworben und bietet die Nutzung auf einem eigenen Server an. BSCW ist das ideale Werkzeug für effiziente Kooperation, insbesondere für firmen- oder standortübergreifende Projekte. Es ermöglicht - ohne Softwareinstallation bei den Endbenutzern - die Zusammenarbeit von beliebig großen Benutzergruppen. Die Anmeldung erfolgt mit softwareeigenen Nutzerdaten, eine Intgration in des zentrale IDM ist nicht umgesetzt.</dcterms:description>
        <dcterms:abstract xml:lang="de">BSCW ist ein bewährtes Werkzeug für effiziente Kooperation, insbesondere für firmen- oder standortübergreifende Projekte.</dcterms:abstract>
        <skos:prefLabel xml:lang="en">BSCW Groupware</skos:prefLabel>
        <schema:url>http://bscw.fh-brandenburg.de</schema:url>
        <skos:prefLabel xml:lang="de">BSCW Groupware</skos:prefLabel>
        <dcterms:title xml:lang="de">BSCW Groupware</dcterms:title>
        <inCategory rdf:resource="&itcat;BusinessCatalog"/>
        <inCategory rdf:resource="&itcat;CommunicationServices"/>
        <usableWith rdf:resource="&itcat;Computer"/>
        <schema:customer rdf:resource="&itcat;DBM"/>
        <usableWith rdf:resource="&itcat;Laptop"/>
        <hasPriority rdf:resource="&itcat;LowPriority"/>
        <supporter rdf:resource="&itcat;MLangerwisch"/>
        <hasCriticality rdf:resource="&itcat;MediumCriticality"/>
        <hasStage rdf:resource="&itcat;Operation"/>
        <usableWith rdf:resource="&itcat;Smartphone"/>
        <user rdf:resource="&itcat;Staff"/>
        <user rdf:resource="&itcat;Students"/>
        <schema:provider rdf:resource="&itcat;TLSO"/>
        <inCategory rdf:resource="&itcat;TeachingPlatforms"/>
        <schema:isRelatedTo rdf:resource="&itcat;WebHosting"/>
    </owl:NamedIndividual>
    


    <!-- http://th-brandenburg.de/ns/itcat#BSNeV -->

