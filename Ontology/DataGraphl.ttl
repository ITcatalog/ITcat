                 :hasCriticality :MediumCriticality ;
                 
                 :inCategory :MiscServices ,
                             :NetworkServices ;
                 
                 :hasStage :Operation ;
                 
                 :usableWith :Smartphone ;
                 
                 :user :StaffADM ,
                       :StaffDBM ;
                 
                 :supporter :TJacob ;
                 
                 schema:provider :TLSO ;
                 
                 :usableWith :Tablet ;
                 
