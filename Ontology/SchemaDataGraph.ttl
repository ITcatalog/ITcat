           rdfs:subPropertyOf schema:agent ;
           
           rdfs:range prov:Agent .



###  http://th-brandenburg.de/ns/itcat#usableWith

:usableWith rdf:type owl:ObjectProperty ;
            
            rdfs:label "usableWith" ;
            
            rdfs:comment "Objektrelation zwischen einem IT-Dienst und der Dienstmerkmalsklasse 'Unterstützung für Endgeräte'."@de ;
            
            skos:definition "Objektrelation zwischen einem IT-Dienst und der Dienstmerkmalsklasse 'Unterstützung für Endgeräte'."@de ;
            
            skos:prefLabel "nutzbar mit"@de ,
                           "usable with"@en ;
            
            rdfs:domain schema:Service ;
            
            rdfs:range :ITAsset .



###  http://th-brandenburg.de/ns/itcat#user

:user rdf:type owl:ObjectProperty ;
      
      rdfs:label "user" ;
      
      rdfs:comment "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Nutzer."@de ;
      
      skos:definition "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Nutzer."@de ;
      
