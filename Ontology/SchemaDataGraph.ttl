      rdfs:subPropertyOf schema:agent ;
      
      rdfs:range prov:Agent .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty ;
             
             rdfs:label "related" ;
             
             skos:prefLabel "Beziehung zwischen Klassen"@de ;
             
