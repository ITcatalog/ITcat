#    Annotation properties
#
#################################################################


###  http://www.purl.org/dc/terms/abstract

dcterms:abstract rdf:type owl:AnnotationProperty ;
                 
                 rdfs:comment "Diese Standardrelation wird in dieser Ontologie verwendet, um Kurzbeschreibungen von IT-Diensten bereitzustellen, die als Teaser im IT-Katalog erscheinen."@de ;
                 
                 skos:prefLabel "Kurzbeschreibung"@de ;
                 
                 skos:definition "Relation zu einer textuellen Kurzbeschreibung einer Ressource."@de ;
                 
                 skos:prefLabel "abstract"@en .



###  http://www.purl.org/dc/terms/description

dcterms:description rdf:type owl:AnnotationProperty ;
                    
                    skos:prefLabel "Beschreibung"@de ;
                    
                    rdfs:comment "Diese Standardrelation wird in dieser Ontologie verwendet, um Beschreibungen von IT-Diensten bereitzustellen."@de ;
                    
                    skos:definition "Im engeren Sinne: Relation zu einer textuellen Beschreibung einer Ressource. Kann im weiteren Sinne auch für andere Formen von Beschreibungen genutzt werden: Grafiken, Inhaltsverzeichnissen etc."@de ;
                    
                    skos:prefLabel "description"@en .



###  http://www.purl.org/dc/terms/subject

dcterms:subject rdf:type owl:AnnotationProperty ;
                
                rdfs:comment "Diese Standardrelation wird in dieser Ontologie verwendet, um Schlüsselwörter zu IT-Diensten zu referenzieren."@de ;
                
                skos:definition "Ein thematischer Gegenstand einer Ressource."@de ;
                
                skos:prefLabel "Gegenstand"@de ,
                               "subject"@en .



###  http://www.purl.org/dc/terms/title

dcterms:title rdf:type owl:AnnotationProperty ;
              
              skos:prefLabel "Bezeichnung"@de ;
              
              skos:definition "Die vollständige (offizielle) Bezeichnung / der Name einer Ressource."@de ;
              
              rdfs:comment "Diese Standardrelation wird in dieser Ontologie verwendet, um vollständige Bezeichnungen von IT-Diensten bereitzustellen."@de ;
              
              skos:prefLabel "title"@en .



###  http://www.w3.org/2004/02/skos/core#altLabel

skos:altLabel rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#definition

skos:definition rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#prefLabel

skos:prefLabel rdf:type owl:AnnotationProperty ;
               
               rdfs:comment "Diese Standardrelation wird in dieser Ontologie verwendet, um primäre Bezeichnungen von Ressourcen bereitzustellen."@de ;
               
               skos:prefLabel "Hauptbezeichnung"@de ;
               
               rdfs:comment "In jeder Sprache kann es immer nur ein skos:prefLabel geben."@de ;
               
               skos:definition "Standardrelation zur Auszeichnung einer Bevorzugten Bezeichnung einer Ressource."@de ;
               
               skos:prefLabel "preferred label"@en .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/goodrelations/v1#hasWarrantyPromise

gr:hasWarrantyPromise rdf:type owl:ObjectProperty .



###  http://schema.org/agent

schema:agent rdf:type owl:ObjectProperty ;
             
             rdfs:label "agent" ;
             
             skos:definition "Abstrakte Objektrelation zwischen einem IT-Dienst und einem Akteur (Instanz der Klasse 'Agent'), der eine handelnde Rolle in Bezug zu diesem IT-Dienst einnimmt, z. B. als Anbieter, Kunde, Nutzer oder Supporter."@de ;
             
             rdfs:comment "Abstrakte Objektrelation zwischen einem IT-Dienst und einem Akteur (Instanz der Klasse 'Agent'), der eine handelnde Rolle in Bezug zu diesem IT-Dienst einnimmt, z. B. als Anbieter, Kunde, Nutzer oder Supporter."@de ;
             
             skos:prefLabel "Akteur"@de ,
                            "agent"@en ;
             
             rdfs:domain schema:Service ;
             
             rdfs:range prov:Agent .



###  http://schema.org/customer

schema:customer rdf:type owl:ObjectProperty ;
                
                rdfs:label "customer" ;
                
                skos:definition "Der Kunde eines IT-Dienstes hat für die Kosten des IT-Dienstes aufzukommen, entweder indem er die Rechnungen des Anbieters begleicht (externer Kunde) oder indem er dem (internen) Anbieter ein Budget zur Verfügung stellt."@de ;
                
                rdfs:comment "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Kunden. Der Kunde hat für die Kosten des IT-Dienstes aufzukommen, entweder indem er die Rechnungen des Anbieters begleicht (externer Kunde) oder indem er dem (internen) Anbieter ein Budget zur Verfügung stellt."@de ;
                
                skos:prefLabel "bestellt von"@de ,
                               "has customer"@en ;
                
                rdfs:domain schema:Service ;
                
                rdfs:subPropertyOf schema:agent ;
                
                rdfs:range prov:Agent .



###  http://schema.org/hasPart

schema:hasPart rdf:type owl:ObjectProperty ;
               
               rdfs:label "hasPart" ;
               
               skos:definition "Objektrelation zwischen zwei Ressourcen, wobei das Subjekt im Vergleich zum Objekt eine übergeordnete Rolle spielt. " ;
               
               rdfs:comment "Objektrelation zwischen zwei Ressourcen, wobei das Subjekt im Vergleich zum Objekt eine übergeordnete Rolle spielt. In dieser Ontologie wird diese Objektrelation vorwiegend verwendet, um eine Verflechtung zwischen einem IT-Asset und einem diese Komponente einsetzenden IT-Dienst, oder zwischen mehreren Diensten abzubilden."@de ;
               
               skos:prefLabel "hat Teil"@de ,
                              "setzt ein"@de ,
                              "has part"@en ;
               
               owl:inverseOf schema:isPartOf .



###  http://schema.org/isPartOf

schema:isPartOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "isPartOf" ;
                
                skos:definition "Objektrelation zwischen zwei Ressourcen, wobei das Subjekt im Vergleich zum Objekt eine untergeordnete Rolle spielt. " ;
                
                rdfs:comment "Objektrelation zwischen zwei Ressourcen, wobei das Subjekt im Vergleich zum Objekt eine untergeordnete Rolle spielt. In dieser Ontologie wird diese Objektrelation vorwiegend verwendet, um eine Verflechtung zwischen einem IT-Asset und einem diese Komponente einsetzenden IT-Dienst, oder zwischen mehreren Diensten abzubilden."@de ;
                
                skos:prefLabel "ist Teil von"@de ,
                               "wird eingesetzt in"@de ,
                               "is part of"@en .



###  http://schema.org/isRelatedTo

schema:isRelatedTo rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "isRelatedTo" ;
                   
                   skos:definition "Objektrelation zwischen zwei Ressourcen (Instanzen), die zueinander in Beziehung stehen."@de ;
                   
                   rdfs:comment "Objektrelation zwischen zwei Ressourcen (Instanzen), die zueinander in Beziehung stehen. Diese Standardrelation kann sehr vielfältig verwendet werden. Zwei Hauptanwendungen in dieser Ontologie sind: 1) die Beziehung zwischen zwei unterschiedlichen Diensten und 2) die Beziehung zwischen Instanzen der Diensmerkmalsklasse 'itcat:UserAssetSupport' und den entsprechenden Instanzen der Klasse 'IT Asset'."@de ;
                   
                   skos:prefLabel "in Beziehung zu"@de ,
                                  "is related to"@en .



###  http://schema.org/provider

schema:provider rdf:type owl:ObjectProperty ;
                
                rdfs:label "provider" ;
                
                rdfs:comment "Objektrelation zwischen einem IT-Dienst und seinem Anbieter."@de ;
                
                skos:definition "Zu unterscheiden sind externe und interne Anbieter von IT-Diensten. Externe Anbieter können Unternehmen oder eine andere Hochschule o. ä. sein. Interne Anbieter sind Einrichtungen der Hochschule selbst, z. B. das Rechenzentrum oder das Labor eines Fachbereichs."@de ;
                
                skos:prefLabel "hat Anbieter"@de ,
                               "has provider"@en ;
                
                rdfs:domain schema:Service ;
                
                rdfs:subPropertyOf schema:agent ;
                
                rdfs:range prov:Agent .



###  http://schema.org/valueReference

schema:valueReference rdf:type owl:ObjectProperty .



###  http://th-brandenburg.de/ns/itcat#hasCriticality

:hasCriticality rdf:type owl:ObjectProperty ;
                
                rdfs:label "hasCriticality" ;
                
                skos:definition "Die Kritikalität eines Dienstes gibt an, wie kritisch eine Störung oder der Ausfall eines Dienstes für die IT-Versorgung einer Organisation eingeschätzt wird. Die Einschätzung erfolgt in drei Abstufungen: hohe, mittlere und geringe Kritikalität."@de ;
                
                rdfs:comment "Objektrelation zwischen einem IT-Dienst und seiner Kritikalitätskategorie. Wird demnächst durch schema:valueReference ersetzt."@de ;
                
                skos:prefLabel "hat Kritikalität"@de ,
                               "has criticality"@en ;
                
                rdfs:range schema:QualitativeValue ;
                
                rdfs:domain schema:Service ;
                
                rdfs:subPropertyOf schema:valueReference .



###  http://th-brandenburg.de/ns/itcat#hasPriority

:hasPriority rdf:type owl:ObjectProperty ;
             
             rdfs:label "hasPriority" ;
             
             skos:definition "Die Priorität eines Dienstes gibt an, welcher Wert ihm im Rahmen des Dienste-Portfolios einer Organisation beigemessen wird. Die Priorität wird in drei Abstufungen dargestellt: hohe, mittlere und geringe Priorität."@de ;
             
             rdfs:comment "Objektrelation zwischen einem IT-Dienst und seiner Prioritätskategorie. Wird demnächst durch schema:valueReference ersetzt."@de ;
             
             skos:prefLabel "hat Priorität"@de ,
                            "has priority"@en ;
             
             rdfs:range schema:QualitativeValue ;
             
             rdfs:domain schema:Service ;
             
             rdfs:subPropertyOf schema:valueReference .



###  http://th-brandenburg.de/ns/itcat#hasStage

:hasStage rdf:type owl:ObjectProperty ;
          
          rdfs:label "hasStage" ;
          
          skos:definition "Gibt an in welcher Phase des Lebenszyklus sich ein IT-Dienst befindet."@de ;
          
          rdfs:comment "Gibt an in welcher Phase des Lebenszyklus sich ein IT-Dienst befindet. Zu verwendende Phasen sind: 'in Planung', 'in Einführung', 'in Betrieb', 'in Ablösung', 'abgeschaltet'."@de ;
          
          skos:prefLabel "hat Lebenszyklusphase"@de ,
                         " has life cycle stage"@en ;
          
          rdfs:domain schema:Service ;
          
          rdfs:range :LifeCycleStage .



###  http://th-brandenburg.de/ns/itcat#hasUtility

:hasUtility rdf:type owl:ObjectProperty ;
            
            rdfs:label "hasUtility" ;
            
            skos:definition "Objektrelation zwischen einem IT-Dienst und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seines Nutzens dient."@de ;
            
            rdfs:comment "Objektrelation zwischen einem IT-Dienst und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seines Nutzens dient. - Noch nicht in Anwendung umgesetzt."@de ;
            
            skos:prefLabel "hat Nutzen"@de ,
                           "has utility"@en ;
            
            rdfs:domain schema:Service ;
            
            rdfs:range usdl-agreement:ServiceProperty .



###  http://th-brandenburg.de/ns/itcat#hasWarranty

:hasWarranty rdf:type owl:ObjectProperty ;
             
             rdfs:label "hasWarranty" ;
             
             skos:definition "Objektrelation zwischen einem IT-Dienst und und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seiner zugesicherten Qualitätsdimensionen (Gewährleistung) dient."@de ;
             
             rdfs:comment "Objektrelation zwischen einem IT-Dienst und und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seiner zugesicherten Qualitätsdimensionen (Gewährleistung) dient. - Noch nicht in Anwendung umgesetzt."@de ;
             
             skos:prefLabel "hat Gewährleistung"@de ,
                            "has warranty"@en ;
             
             rdfs:subPropertyOf gr:hasWarrantyPromise ;
             
             rdfs:domain schema:Service ;
             
             rdfs:range usdl-agreement:ServiceProperty .



###  http://th-brandenburg.de/ns/itcat#inCategory

:inCategory rdf:type owl:ObjectProperty ;
            
            rdfs:label "inCategory" ;
            
            rdfs:comment "Aktuell werden zwei Kategorieklassen unterschieden: Sachkategorien und Katalogkategorien. Jeder IT-Dienst muss mindestens einer Kategorie in jeder Kategorieklasse zugeordnet sein. Ein IT-Dienst kann gleichzeitig mehreren Dienstekategorien innerhalb einer Kategorieklasse angehören."@de ;
            
            skos:definition "Objektrelation zwischen einem IT-Dienst und einer Dienstekategorie. "@de ;
            
            skos:prefLabel "gehört zu Kategorie"@de ,
                           "belongs to category"@en ;
            
            rdfs:domain schema:Service ;
            
            rdfs:range :ServiceCategory .



###  http://th-brandenburg.de/ns/itcat#supporter

:supporter rdf:type owl:ObjectProperty ;
           
           rdfs:label "supporter" ;
           
           skos:definition "Der Suppporter eines IT-Dienstes ist die Person oder Organisation, die Wartung, Pflege und/oder Nutzerbetreuung für den IT-Dienst übernimmt. Zumeist handelt es sich um einen verantwortlich Handelnden aus der Hochschule selbst."@de ;
           
           rdfs:comment "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Supporter."@de ;
           
           skos:prefLabel "hat Betreuer"@de ,
                          "has supporter"@en ;
           
           rdfs:domain schema:Service ;
           
           rdfs:subPropertyOf schema:agent ;
           
           rdfs:range prov:Agent .



###  http://th-brandenburg.de/ns/itcat#usableWith

:usableWith rdf:type owl:ObjectProperty ;
            
            rdfs:label "usableWith" ;
            
            skos:definition "Gibt an, mit welchen Endgeräten ein IT-Dienst genutzt werden kann."@de ;
            
            rdfs:comment "Objektrelation zwischen einem IT-Dienst und der Dienstmerkmalsklasse 'Unterstützung für Endgeräte'."@de ;
            
            skos:prefLabel "nutzbar mit"@de ,
                           "usable with"@en ;
            
            rdfs:domain schema:Service ;
            
            rdfs:range :ITAsset .



###  http://th-brandenburg.de/ns/itcat#user

:user rdf:type owl:ObjectProperty ;
      
      rdfs:label "user" ;
      
      skos:definition "Nutzer von IT-Diensten sind Personen, die berechtigt sind, die IT-Dienste für ihre fachlichen Aufgaben an der Hochschule (Fachanwender) oder für ihre privaten Informationsinteressen (Endanwender) in Anspruch zu nehmen. Die Nutzer werden als Rollen dargestellt, z. B. Verwaltungsmitarbeiter, Studierende, Lehrende."@de ;
      
      rdfs:comment "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Nutzer."@de ;
      
      skos:prefLabel "hat Nutzer"@de ,
                     "has user"@en ;
      
      rdfs:domain schema:Service ;
      
      rdfs:subPropertyOf schema:agent ;
      
      rdfs:range prov:Agent .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty ;
             
             rdfs:label "related" ;
             
             skos:prefLabel "Beziehung zwischen Klassen"@de ;
             
             skos:definition "Objektrelation zwischen zwei Klassen, die zueinander in Beziehung stehen."@de ;
             
             rdfs:comment "Objektrelation zwischen zwei Klassen, die zueinander in Beziehung stehen. Diese Standardrelation wird hier verwendet um nicht näher spezifizierbare Beziehungen zwischen proprietären itcat-Klassen und Standardklassen zu erklären."@de ;
             
             skos:prefLabel "related"@en .



###  http://xmlns.com/foaf/0.1/page

foaf:page rdf:type owl:ObjectProperty ;
          
          rdfs:label "page" ;
          
          rdfs:comment "Objektrelation zwischen einer Ressource, z. B. einem IT-Dienst und einem Dokument, das Informationen für oder über diesen IT-Dienst enthält."@de ;
          
          skos:definition "Objektrelation zwischen einer Ressource, z. B. einem IT-Dienst und einem Dokument, das Informationen für oder über diesen IT-Dienst enthält."@de ;
          
          skos:prefLabel "hat Dokument"@de ,
                         "has page"@en ;
          
          rdfs:range foaf:Document ;
          
          owl:inverseOf foaf:topic .



###  http://xmlns.com/foaf/0.1/topic

foaf:topic rdf:type owl:ObjectProperty ;
           
           rdfs:label "topic" ;
           
           skos:definition "Objektrelation zwischen einem Dokument und einer anderen Ressource. Dokumente können Beschreibungen, Benutzerhilfen, Regeln, Richtlinien, Prozessmodelle, Verträge o. ä. sein bzw. enthalten, die einen Bezug zu einem IT-Dienst haben."@de ;
           
           rdfs:comment "Objektrelation zwischen einem Dokument und einer anderen Ressource. Dokumente können Beschreibungen, Benutzerhilfen, Regeln, Richtlinien, Prozessmodelle, Verträge o. ä. sein bzw. enthalten, die einen Bezug zu einem IT-Dienst haben."@de ;
           
           skos:prefLabel "hat Thema"@de ,
                          "has topic"@en ;
           
           rdfs:domain foaf:Document .





#################################################################
#
#    Data properties
#
#################################################################


###  http://schema.org/url

schema:url rdf:type owl:DatatypeProperty ;
           
           rdfs:label "url" ;
           
           skos:definition "Referenziert eine URL zu einer Ressource. Verweist auf weiterführende Informationen, Dokumente, Zugänge etc."@de ;
           
           rdfs:comment "Referenziert eine URL zu einer Ressource. Verweist auf weiterführende Informationen, Dokumente, Zugänge etc. Diese Standardrelation wird in dieser Ontologie verwendet, um die bereits vorhandenen Webseiten zu den IT-Diensten zu referenzieren."@de ;
           
           skos:prefLabel "URL"@de ,
                          "url"@en .





#################################################################
#
#    Classes
#
#################################################################


###  http://schema.org/QualitativeValue

schema:QualitativeValue rdf:type owl:Class ;
                        
                        rdfs:label "QualitativeValue" ;
                        
                        rdfs:comment "Qualitative Werte erlauben die Beurteilung eines Dienstes oder einer Entität im Hinblick auf ihren Wert/ihre Bedeutung für die Organisation. In dieser Ontologie stehen IT-Dienste im Fokus. Qualitative Werte beziehen sich hauptsächlich auf IT-Dienste und ihren Wert/ihre Bedeutung für die Organisation." ;
                        
                        skos:definition "Qualitative Werte erlauben die Beurteilung eines Dienstes oder einer Entität im Hinblick auf ihren Wert/ihre Bedeutung für die Organisation."@de ;
                        
                        skos:prefLabel "qualitativer Wert"@de ,
                                       "qualitative value"@en .



###  http://schema.org/Role

schema:Role rdf:type owl:Class ;
            
            rdfs:label "Role" ;
            
            rdfs:subClassOf prov:Agent ;
            
            rdfs:comment "Eine Rolle im Rahmen dieser Ontologie meint ein Handlungsmodell in Bezug auf IT-Dienste der Hochschule. Wesentliche Rollen sind Mitarbeiter (verschiedener Organisationseinheite), Studierende, Interessenten etc."@de .



###  http://schema.org/Service

schema:Service rdf:type owl:Class ;
               
               rdfs:label "Service" ;
               
               skos:prefLabel "Dienst"@de ;
               
               skos:definition "Ein IT-Dienst ist eine abgrenzbare Dienstleistung, die auf dem Einsatz von Informationstechnologie basiert. Sie wird von einem Leistungsanbieter (IT Service Provider) für interne und/oder externe Leistungsempfänger bereitgestellt."@de ;
               
               rdfs:comment "Gegenstand dieser Ontologie sind IT-Dienste, klassifiziert nach Katalogen und Sachkategorien sowie die Relationen zwischen IT-Diensten, Empfängern, Anbietern sowie Technologiekomponenten. Ein IT-Dienst ist eine abgrenzbare Dienstleistung, die auf dem Einsatz von Informationstechnologie basiert. Sie wird von einem Leistungsanbieter (IT Service Provider) für interne und/oder externe Leistungsempfänger bereitgestellt."@de ;
               
               skos:prefLabel "service"@en .



###  http://th-brandenburg.de/ns/itcat#CatalogCategory

:CatalogCategory rdf:type owl:Class ;
                 
                 rdfs:label "CatalogCategory" ;
                 
                 rdfs:subClassOf :ServiceCategory ;
                 
                 skos:prefLabel "Katalogkategorie"@de ;
                 
                 skos:definition "Katalogkategorien helfen bei der Einordnung von IT-Diensten in verschiedene Dienstekataloge."@de ;
                 
                 rdfs:comment "Katalogkategorien helfen bei der Einordnung von IT-Diensten in verschiedene Dienstekataloge. Wie IT-Dienste in Katalogkategorien eingruppiert werden, ist noch nicht abschließend geklärt. Aktuell sind vier Katalogkategorien angelegt: Fachanwenderkatalog, IT-Betriebskatalog, IT-Infrastrukturkatalog und Öffentlicher Dienstekatalog."@de ;
                 
                 skos:prefLabel "catalog category"@en .



###  http://th-brandenburg.de/ns/itcat#Criticality

:Criticality rdf:type owl:Class ;
             
             rdfs:label "Criticality" ;
             
             rdfs:subClassOf schema:QualitativeValue ;
             
             skos:definition "Die Kritikalität eines Dienstes gibt an, wie kritisch eine Störung oder der Ausfall eines Dienstes für die IT-Versorgung einer Organisation eingeschätzt wird."@de ;
             
