@prefix : <http://th-brandenburg/ns/itcat#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dcterms: <http://www.purl.org/dc/terms/>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix gr: <http://purl.org/goodrelations/v1#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix itcat: <http://th-brandenburg/ns/itcat#> .
@prefix usdl-core: <http://www.linked-usdl.org/ns/usdl-core#> .
@prefix usdl-agreement: <http://www.linked-usdl.org/ns/usdl-agreement#> .
@base <http://th-brandenburg/ns/itcat#> .

<http://th-brandenburg/ns/itcat#> rdf:type owl:Ontology ;
                                  
                                  rdfs:label "ITCat" ;
                                  
                                  rdfs:comment "Ontologie für den IT-Dienste-Katalog einer Organisation, die sowohl externe IT-Dienste nutzt, als auch eigene IT-Dienste bereitstellt. "@de ;
                                  
                                  owl:versionInfo "v 0.1" .


#################################################################
#
#    Classes
#
#################################################################


###  http://th-brandenburg.de/ns/itcat#ITService

itcat:ITService rdf:type owl:Class ;
             rdfs:subClassOf schema:Product; ###( owl:unionOf (prov:Agent prov:Entity prov:Activity )) ;
			 skos:related usdl-core:Service ; 	 
           
             skos:prefLabel "IT-Dienst"@de ;
             rdfs:label "IT Service"@en ;
             skos:definition "Ein IT-Dienst ist eine abgrenzbare Dienstleistung, die auf dem Einsatz von Informationstechnologie basiert. Sie wird von einem Leistungserbringer (IT Service Provider) für interne und/oder externe Leistungsempfänger bereitgestellt."@de .


###  http://th-brandenburg/ns/itcat#Agent

itcat:Agent rdf:type owl:Class ;
			owl:equivalentClass prov:Agent ;
            
            rdfs:label "Agent"@en ;
            skos:prefLabel "Akteur"@de ; 
            skos:definition "Ein Akteur ist eine Person, Organisation oder eine Rolle, die als Leistungsanbieter, Leistungsempfänger, Nutzer oder sonstiger Verantwortlicher im Rahmen eines IT-Dienstes auftritt."@de .


###  http://th-brandenburg/ns/itcat#Costs

itcat:Costs rdf:type owl:Class ;
            
            rdfs:label "Costs"@de ;
            skos:prefLabel "Kosten"@de ;
  			skos:definition "Kosten stellen den Werteverzehr durch einen IT-Dienst dar. Sie werden nach Kostenarten erfasst und mit Bezug auf eine Leistungseinheit und eine Zeiteinheit verrechnet. "@de .


###  http://th-brandenburg/ns/itcat#Customer

itcat:Customer rdf:type owl:Class ;
            rdfs:subClassOf itcat:Agent , prov:Agent ;
              
            skos:prefLabel "Kunde"@de ;
            rdfs:label "Customer"@en ;               
            skos:definition "Der Kunde eines Dienstes ist der Vertragspartner des Dienste-Anbieters (Providers). Er kann, muss aber nicht auch selbst Nutzer des Dienstes sein."@de .


###  http://th-brandenburg/ns/itcat#Depreciation

itcat:Depreciation rdf:type owl:Class ;
            rdfs:subClassOf itcat:InternalCosts ;
                   
            skos:prefLabel "Abschreibung"@de ;
            rdfs:label "Depreciation"@en ;                 
            skos:definition "Abschreibungen ermitteln sich in diesem Kontext aus der anteilig einem Dienst zuzurechnenden Nutzung von Vermögensgegenständen einer Organisation. Vermögensgegenstände können sowohl materieller (z. B. Server-Platten) als auch immaterieller Natur (z. B. Lizenzen) sein."@de .


###  http://th-brandenburg/ns/itcat#Document

itcat:Document rdf:type owl:Class ;
			owl:equivalentClass foaf:Document;
			rdfs:subClassOf prov:Entity;
			               
            skos:prefLabel "Dokument"@de ;
            rdfs:label "Document"@en ; 
            skos:definition "Dokumente beinhalten unstrukturierte bzw. schwach strukturierte Informationen in elektronischer oder papiergebundener Form. Sie sind primär darauf ausgerichtet, von Menschen gelesen zu werden. Durch den Einsatz semantischer Technologien kann die maschinelle Auswertbarkeit von elektronischen Dokumenten sognifikant erhöht werden."@de .


###  http://th-brandenburg/ns/itcat#ExternalCosts

itcat:ExternalCosts rdf:type owl:Class ;
			rdfs:subClassOf itcat:Costs ;
			
            rdfs:label "External Costs"@en ;
            skos:prefLabel "Externe Kosten"@de;
  			skos:definition "Externe Kosten eines IT-Dienstes fallen gegenüber externen Providern an. Sie werden zu vereinbarten Zeiten auf Grundlage von Rechnungen oder Rahmenvereinbarungen gezahlt."@de .


###  http://th-brandenburg/ns/itcat#ITComponent

itcat:ITComponent rdf:type owl:Class ;
            rdfs:subClassOf prov:Entity ;
            
            skos:prefLabel "IT-Komponente"@de ;
            rdfs:label "IT Component"@en ;
            skos:definition "IT-Komponenten sind die im Rahmen eines IT-Dienstes eingesetzten Systeme und Komponenten der Informations- und Kommunikationstechnologie. Dazu zählen u. a. PCs, Server, Drucker, Netzwerkkomponenten, Software(-Pakete). "@de ;
			rdfs:comment "Die IT-Komponenten gelten im Rahmen des IT-Service-Managements als Configuration Items. Da dieser Begriff jedoch viel weiter gefasst ist und neben technischen Komponenten auch Dokumente, Prozesse, Rollen und auch Dienste umfasst, soll hier der engere Begriff IT-Komponenten verwendet werden."@de .


###  http://th-brandenburg/ns/itcat#InternalCosts

itcat:InternalCosts rdf:type owl:Class ;
            rdfs:subClassOf itcat:Costs ;        
            
            skos:prefLabel "Interne Kosten"@de ;
            rdfs:label "Internal Costs"@en ;
            skos:definition "Interne Kosten repräsentieren den Ressourcenverbrauch innerhalb einer Organisation. Sie werden durch interne Verrechnungen ermittelt."@de .


###  http://th-brandenburg/ns/itcat#OperationalCosts

itcat:OperationalCosts rdf:type owl:Class ;
             rdfs:subClassOf itcat:InternalCosts ;
             
             skos:prefLabel "Betriebskosten"@de ;
             rdfs:label "Operational Costs"@en ;
             skos:definition "Betriebskosten ermitteln sich aus dem anteilig einem Dienst zuzurechnenden Verbrauche von Betriebsstoffe und Kleinmaterial. Zur Vereinfachung sollten auch eventuelle sonstige Kosten hier erfasst werden."@de .


###  http://th-brandenburg/ns/itcat#Provider

itcat:Provider rdf:type owl:Class ;
             rdfs:subClassOf itcat:Agent , prov:Agent ;
             
             skos:prefLabel "Anbieter"@de ;
             rdfs:label "Provider"@en ;
             skos:definition "Der Anbieter eines Dienstes ist eine Person oder Organisation, die Leistungen im Rahmen des Dienstes bereitstellt. Provider kann auch eine Organisationseinheit innerhalb einer Organisation sein, z. B. ein internes Rechenzentrum oder eine IT-Abteilung."@de .

###  http://th-brandenburg/ns/itcat#ServiceEvaluation

itcat:ServiceEvaluation rdf:type owl:Class ;
             rdfs:subClassOf gr:QualitativeValue ;
             
             skos:prefLabel "Dienstebewertung"@de ;
             rdfs:label "Service Evaluation"@en ;
             skos:definition "Dienstbwertungen sind sind qualitative Charakteristika der Dienste im Hinblick auf ihren Wert/ihre Bedeutung für die Organisation."@de .

###  http://th-brandenburg/ns/itcat#Priority

itcat:Priority rdf:type owl:Class ;
             rdfs:subClassOf itcat:ServiceEvaluation ;
             
             skos:prefLabel "Priorität"@de ;
             rdfs:label "Priority"@en ;
             skos:definition "Die Priorität eines Dienstes gibt an, welcher Wert ihm im Rahmen des Dienste-Portfolios einer Organisation beigemessen wird."@de .
             
###  http://th-brandenburg/ns/itcat#Criticality

itcat:Criticality rdf:type owl:Class ;
             rdfs:subClassOf itcat:ServiceEvaluation ;
             
             skos:prefLabel "Kritikalität"@de ;
             rdfs:label "Criticality"@en ;
             skos:definition "Die Kritikalität eines Dienstes gibt an, wie kritisch eine Störung oder der Ausfall eines Dienstes für die IT-Versorgung einer Organisation eingeschätzt wird."@de .

###  http://th-brandenburg/ns/itcat#ServiceOption

itcat:ServiceOption rdf:type owl:Class ;
             rdfs:subClassOf schema:OfferItemOption ;
             
             skos:prefLabel "Dienstoption"@de ;
             rdfs:label "Service Option"@en ;
             skos:definition "Dienstoptionen sind unterscheidbare Pakete von Funktionalitäten und Gewährleistungen im Hinblick auf einen Dienst, die von Kunden ausgewählt werden können und die mit einem (Verrechnungs-)Preis versehen sind."@de .


###  http://th-brandenburg/ns/itcat#ServiceProperty

itcat:ServiceProperty rdf:type owl:Class ;
             owl:equivalentClass usdl-agreement:ServiceProperty ;
             
             skos:prefLabel "Dienstmerkmal"@de ;
             rdfs:label "Service Property"@en ;
             skos:definition "Dienstmerkmale sind charakteristische Eigenschaften eines IT-Dienstes, die seinen Nutzen/seine Funktionalität sowie seine zugesicherten Qualitätsmerkmale darstellen."@de .


###  http://th-brandenburg/ns/itcat#StaffCosts

itcat:StaffCosts rdf:type owl:Class ;
             rdfs:subClassOf itcat:InternalCosts ;
             
             skos:prefLabel "Personalkosten"@de ;
             rdfs:label "Staff costs"@en ;
             skos:definition "Personalkosten umfassen die anteilig auf einen Dienst anzurechnenden Kosten für den Einsatz internen Personals. Hier sollten auch zurechenbare Anteile von Reise- und Fortbildungskosten erfasst werden."@de .


###  http://th-brandenburg/ns/itcat#Supporter

itcat:Supporter rdf:type owl:Class ;
             rdfs:subClassOf itcat:Agent , prov:Agent ;   
             
             skos:prefLabel "Supporter"@de ;
             rdfs:label "Supporter"@en ;
             skos:definition "Supporter ist jede Person, Organisation oder Rolle, die Nutzer eines Dienstes bei Fragen oder Problemen unterstützt, Fehler behebt bzw. Hilfe leistet."@de .


###  http://th-brandenburg/ns/itcat#User

itcat:User rdf:type owl:Class ;
            rdfs:subClassOf itcat:Agent , prov:Agent ;
            
            skos:prefLabel "Nutzer"@de ;
            rdfs:label "User"@en ;
            skos:definition "Der Nutzer eines Dienstes ist der direkte Empfänger der Leistungen. Er kann, muss aber nicht gleichzeitig Kunde des Dienstes sein."@de ;
            rdfs:comment "Nutzer können externe oder interne Personen oder Gruppen der Organisation sein"@de .


###  http://th-brandenburg/ns/itcat#Utility

itcat:Utility rdf:type owl:Class ;
            rdfs:subClassOf itcat:ServiceProperty ; 
            
            skos:prefLabel "Funktion"@de ;
            rdfs:label "Utility"@en ;
            skos:altLabel "Nutzen"@de ;
            skos:definition "Der Nutzen/die Funktion eines Dienstes stellt die Art der Leistung dar, die der IT-Dienst dem Nutzer bietet, z. B. Mails senden und empfangen, Kalender und Aufgaben verwalten, Lehrvideos bereitstellen, Dokumente austauschen."@de ;
            rdfs:comment "Der Nutzen/die Funktion eines Dienstes beantwortet die Frage: 'Was leistet der Dienst für den Nutzer?'"@de ;
            rdfs:comment "Der Nutzen kann in Form einer informellen Beschreibung, einer formalen (technischen) Spezifikation oder in Form eines juristischen Dokumentes (Vertrag) niedergelegt sein."@de .                           


###  http://th-brandenburg/ns/itcat#Warranty

itcat:Warranty rdf:type owl:Class ;
            rdfs:subClassOf itcat:ServiceProperty ;
            
            skos:prefLabel "Gewährleistung"@de ;
            rdfs:label "Warranty"@en ;
            skos:definition "Die Gewährleistung umfasst alle Zusicherungen des Dienste-Anbieters in Bezug auf Qualitätsdimensionen des Dienstes, z. B. Verfügbarkeit, Bandbreite, Geschwindigkeit, Transfervolumen, Verschlüsselungsgrad."@de ;
            rdfs:comment "Gewährleistungen sollten in einer Vereinbarung zwischen Dienste-Anbieter und Kunde festgehalten werden. Solche Vereinbarungen werden im IT-Service-Management als Service Level Agreement (SLA, für externe Kunden) bzw. Operational Level Agreement (OLA, für interne Kunden) bezeichnet. "@de .



#################################################################
#
#    Object Properties
#
#################################################################

itcat:actsIn rdf:type owl:ObjectProperty ;
			skos:prefLabel "agiert in"@de ;
			rdfs:label "acts in"@en ;
			skos:definition "Abstrakte Objektrelation zwischen einem Akteur (Instanz der Klasse 'Agent') und einem IT-Dienst. Bezeichnet eine handelnde Rolle in Bezug zu diesem IT-Dienst, z. B. als Anbieter, Kunde, Nutzer oder Supporter."@de ;
			rdfs:domain itcat:Agent ;
			rdfs:range itcat:ITService .
			
itcat:hasActor rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Akteur"@de ;
			rdfs:label "has actor"@en ;
			skos:definition "Abstrakte Objektrelation zwischen einem IT-Dienst und einem Akteur (Instanz der Klasse 'Agent'), der eine handelnde Rolle in Bezug zu diesem IT-Dienst einnimmt, z. B. als Anbieter, Kunde, Nutzer oder Supporter."@de ;
			owl:inverseOf itcat:actsIn .
			
itcat:orders rdf:type owl:ObjectProperty ;
			skos:prefLabel "bestellt"@de ;
			rdfs:label "orders"@en ;
			skos:definition "Objektrelation zwischen einem Kunden und einem IT-Dienst, den der Kunde in Auftrag gegeben (bestellt) hat. Der Kunde hat für die Kosten des IT-Dienstes aufzukommen, entweder indem er die Rechnungen des Anbieters begleicht (externer Kunde) oder indem er dem (internen) Anbieter ein Budget zur Verfügung stellt."@de ;
			rdfs:domain itcat:Customer ;
			rdfs:range itcat:ITService .
			
itcat:isOrderedBy rdf:type owl:ObjectProperty ;
			skos:prefLabel "bestellt von"@de ;
			rdfs:label "is ordered by"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und (einem) seinem(r) Kunden. Der Kunde hat für die Kosten des IT-Dienstes aufzukommen, entweder indem er die Rechnungen des Anbieters begleicht (externer Kunde) oder indem er dem (internen) Anbieter ein Budget zur Verfügung stellt."@de ;
			owl:inverseOf itcat:orders . 

itcat:provides rdf:type owl:ObjectProperty ;
			skos:prefLabel "bietet an"@de ;
			rdfs:label "provides"@en ;
			skos:definition "Objektrelation zwischen dem Anbieter eines IT-Dienstes und dem betreffenden IT-Dienst."@de ;
			rdfs:domain itcat:Provider ;
			rdfs:range itcat:ITService .
			
itcat:isProvidedBy rdf:type owl:ObjectProperty ;
			skos:prefLabel "wird angeboten von"@de ;
			rdfs:label "is provided by"@en ;
			skos:definition "Objektrelation zwischen dem einem IT-Dienstes und seinem jeweiligen Anbieter."@de ;
			owl:inverseOf itcat:provides . 

itcat:uses rdf:type owl:ObjectProperty ;
			skos:prefLabel "nutzt"@de ;
			rdfs:label "uses"@en ;
			skos:definition "Objektrelation zwischen dem Nutzer eines IT-Dienstes und dem betreffenden IT-Dienst."@de ;
			rdfs:domain itcat:User ;
			rdfs:range itcat:ITService .
			
itcat:isUsedBy rdf:type owl:ObjectProperty ;
			skos:prefLabel "wird genutzt von"@de ;
			rdfs:label "is used by"@en ;
			skos:definition "Objektrelation zwischen dem einem IT-Dienstes und (einem) seinem(r) Nutzer."@de ;
			owl:inverseOf itcat:uses . 

itcat:isCausedBy rdf:type owl:ObjectProperty ;
			skos:prefLabel "wird verursacht durch"@de ;
			rdfs:label "is caused by"@en ;
			skos:definition "Objektrelation zwischen einer Kostenart und einem diese Kosten verursachenden IT-Dienst."@de ;
			rdfs:domain itcat:Costs ;
			rdfs:range itcat:ITService .
			
itcat:causes rdf:type owl:ObjectProperty ;
			skos:prefLabel "verursacht"@de ;
			rdfs:label "causes"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und einer Kostenart, die durch diesen IT-Dienst verursacht wird."@de ;
			owl:inverseOf itcat:isCausedBy .
			
itcat:isEmployedBy rdf:type owl:ObjectProperty ;
			skos:prefLabel "wird eingesetzt in"@de ;
			rdfs:label "is employed by"@en ;
			skos:definition "Objektrelation zwischen einer IT-Komponente und einem diese Komponente einsetzenden IT-Dienst."@de ;
			rdfs:domain itcat:ITComponent ;
			rdfs:range itcat:ITService .
			
itcat:employs rdf:type owl:ObjectProperty ;
			skos:prefLabel "setzt ein"@de ;
			rdfs:label "employs"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und einer IT-Komponente, die für diesen IT-Dienst eingesetzt wird."@de ;
			owl:inverseOf itcat:isEmployedBy .

itcat:isLinkedTo rdf:type owl:ObjectProperty ;
			skos:prefLabel "ist verknüpft mit"@de ;
			rdfs:label "is linked to"@en ;
			skos:definition "Objektrelation zwischen einem Dokument und einem IT-Dienst. Dokumente können Beschreibungen, Regeln, Richtlinien, Prozessmodelle o. ä. enthalten, die einen Bezug zu dem IT-Dienst haben."@de ;
			rdfs:domain itcat:Document ;
			rdfs:range itcat:ITService .
			
itcat:linksTo rdf:type owl:ObjectProperty ;
			skos:prefLabel "verweist auf"@de ;
			rdfs:label "links to"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und einem Dokument, das Informationen für oder über diesen IT-Dienst enthält."@de ;
			owl:inverseOf itcat:isLinkedTo .
			
itcat:hasOfferSpecification rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Angebotsspezifikation"@de ;
			rdfs:label "has offer specification"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und einer/seiner spezifischen Dienstoption, die unmittelbar von Kunden ausgewählt werden kann. "@de ;
			rdfs:domain itcat:ITService ;
			rdfs:range itcat:ServiceOption .
			
itcat:specifiesOfferOf rdf:type owl:ObjectProperty ;
			skos:prefLabel "spezifiziert Angebot von"@de ;
			rdfs:label "specifies offer of"@en ;
			skos:definition "Objektrelation zwischen einer spezifischen, von Kunden unmittelbar auswählbaren Dienstoption eines IT-Dienst und diesem IT-Dienst."@de ;
			owl:inverseOf itcat:hasOfferSpecification .
			
itcat:hasCriticality rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Kritikalität"@de ;
			rdfs:label "has criticality"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und seiner Kritikalitätsklasse. "@de ;
			rdfs:domain itcat:ITService ;
			rdfs:range itcat:ServiceEvaluation .
			
itcat:hasPriority rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Priorität"@de ;
			rdfs:label "has priority"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und seiner Prioritätsklasse. "@de ;
			rdfs:domain itcat:ITService ;
			rdfs:range itcat:ServiceEvaluation .
			
itcat:hasUtility rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Nutzen"@de ;
			rdfs:label "has utility"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seines Nutzens dient."@de ;
			rdfs:domain itcat:ITService ;
			rdfs:range itcat:ServiceProperty .

itcat:hasWarranty rdf:type owl:ObjectProperty ;
			skos:prefLabel "hat Gewährleistung"@de ;
			rdfs:label "has warranty"@en ;
			skos:definition "Objektrelation zwischen einem IT-Dienst und und einer Dienstmerkmalsklasse, die der Beschreibung/Spezifikation seiner zugesicherten Qualitätsdimensionen (Gewährleistung) dient."@de ;
			rdfs:domain itcat:ITService ;
			rdfs:range itcat:ServiceProperty .

