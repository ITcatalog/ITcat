<http://th-brandenburg/ns/itcat#> rdf:type owl:Ontology ;
                                  
                                  rdfs:label "ITCat" ;
                                  
                                  rdfs:comment "Ontologie für den IT-Dienste-Katalog einer Organisation, die sowohl externe IT-Dienste nutzt, als auch eigene IT-Dienste bereitstellt. "@de ;
                                  
                                  owl:versionInfo "v 0.1" .


#################################################################
