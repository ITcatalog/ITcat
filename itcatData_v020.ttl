        itcat:hasCriticality	itcat:MediumCriticality ;
        itcat:hasPriority		itcat:LowPriority ;
        itcat:usableWith  		itcat:Smartphone , itcat:Laptop , itcat:Computer ;
        schema:isRelatedTo		itcat:WebHosting ;
        foaf:page 				itcat:CloudserviceFBW_DocDL001 , itcat:CloudserviceFBW_DocWP001 ;
        itcat:inCategory		itcat:MiscServices , itcat:NetworkServices , itcat:BusinessCatalog .
        

