        foaf:page 				"http://fbwcms.fh-brandenburg.de/sixcms/media.php/1127/filr1_qs_mobile.pdf" , "http://itworks.fh-brandenburg.de/" ;
        itcat:inCategory		itcat:MiscServices , itcat:NetworkServices , itcat:BusinessCatalog .
        

