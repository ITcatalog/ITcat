itcat:BSCW  a                   schema:Service ;
		#Annotation Properties
		skos:prefLabel			"BSCW Groupware"@de ;
		skos:prefLabel			"BSCW Groupware"@en ;
		dcterms:title			"BSCW Groupware"@de ;
		dcterms:abstract		"BSCW ist ein bewährtes Werkzeug für effiziente Kooperation, insbesondere für firmen- oder standortübergreifende Projekte."@de ;
        dcterms:description     "BSCW ist ein Projekt- und Dokumentenmanagementsystem (Basic Support for Cooperative Work). Es wurde vor 20 Jahren vom Fraunhofer Institut FIT entwickelt. Inzwischen zeichnet die OrbiTeam Software GmbH & Co. KG für die Pflege, Weiterentwicklung und den Support des Systems verantwortlich. Der FB Wirtschaft hat eine Lizenz für Forschung und Lehre erworben und bietet die Nutzung auf einem eigenen Server an. BSCW ist das ideale Werkzeug für effiziente Kooperation, insbesondere für firmen- oder standortübergreifende Projekte. Es ermöglicht - ohne Softwareinstallation bei den Endbenutzern - die Zusammenarbeit von beliebig großen Benutzergruppen. Die Anmeldung erfolgt mit softwareeigenen Nutzerdaten, eine Intgration in des zentrale IDM ist nicht umgesetzt."@de ;
        dcterms:subject         "" ;
        rdfs:comment			"" ;
        #Data Properties
        schema:url              "http://bscw.fh-brandenburg.de" ;
        itcat:status             "in Betrieb" ;
        #Object Properties
        itcat:user				itcat:Staff , itcat:Students ;
        schema:customer			itcat:DBM ;
        schema:provider			itcat:TLSO ;
        itcat:supporter			itcat:MLangerwisch ;
        itcat:hasCriticality	itcat:MediumCriticality ;
        itcat:hasPriority		itcat:LowPriority ;
        itcat:usableWith  		itcat:Smartphone , itcat:Laptop , itcat:Computer ;
        schema:isRelatedTo		itcat:WebHosting ;
        foaf:page 				"" ;
        itcat:inCategory		itcat:TeachingPlatforms , itcat:CommunicationServices , itcat:BusinessCatalog .
         

itcat:CloudserviceFBW  a        schema:Service ;
		#Annotation Properties
		skos:prefLabel			"Cloudservice des FBW"@de ;
		skos:prefLabel			"Cloudservice DBM"@en ;
		dcterms:title			"Cloudservice des Fachbereichs Wirtschaft"@de ;
		dcterms:abstract		"Den Mitarbeiterinnen und Mítarbeitern des Fachbereichs Wirtschaft und der Verwaltung steht nunmehr auch ein Cloud-Service der Firma Novel zur Verfügung. Der Hersteller bewirbt diesen Dienst mit den Worten 'Ähnlich wie Dropbox. Sicher wie Fort Knox.'."@de ;
        dcterms:description     "<p>Unseren Cloud-Service (Filr) erreichen Sie am besten mit einem Browser über die u. a. URL. Bitte loggen Sie sich dort mit Ihrem FHB-Account ein. Die Startseite zeigt nur den Mitarbeiterinnen und Mitarbeitern des Fachbereichs Wirtschaft das vom Novell-Client bekannte Home-Laufwerk, welches Sie als Arbeitsbereich nutzen können. Des Weiteren sehen Sie unter dem Menüpunkt Netzlaufwerke, die Ihnen zugewiesenen Laufwerke (z.B. Projekte, Lehre etc.).</p><p>Über die Weboberfläche unseres Cloud-Services können Sie sich zusätzlich einen Client für Ihren PC/MAC herunterladen. Dieser Client besitzt erweiterte Funktionalitäten, z.B. auch Synchronisationsmöglichkeiten, wie Sie sie vom iFolder her kennen. (Klicken Sie in der Webanwendung oben rechts auf Ihren Namen. Es öffnet sich ein Tooltip mit einem Link zum Download.) Für Smartphones und Tablets gibt es im Apple App Store bzw. Google Play Store eine kostenlose App mit dem Namen 'Filr'.</p>"@de ;
        dcterms:subject         "" ;
        rdfs:comment			"" ;
        #Data Properties
        schema:url              "https://cloud.fh-brandenburg.de" ;
        itcat:status             "in Betrieb" ;
        #Object Properties
        itcat:user				itcat:StaffDBM , itcat:StaffAdmin ;
        schema:customer			itcat:DBM ;
        schema:provider			itcat:TLSO ;
        itcat:supporter			itcat:TJacob ;
        itcat:hasCriticality	itcat:MediumCriticality ;
        itcat:hasPriority		itcat:LowPriority ;
        itcat:usableWith  		itcat:Smartphone , itcat:Laptop , itcat:Computer ;
        schema:isRelatedTo		itcat:WebHosting ;
        foaf:page 				"http://fbwcms.fh-brandenburg.de/sixcms/media.php/1127/filr1_qs_mobile.pdf" , "http://itworks.fh-brandenburg.de/" ;
        itcat:inCategory		itcat:MiscServices , itcat:NetworkServices , itcat:BusinessCatalog .
        
        
